@{
    ViewBag.Title = "Sonuc";
}
@using softptoject.App_Classes
@model List<MaterialC>
@section head{

    <li class="menu-item-top" style="float: left">
        <a href="/Uye/Profil" class="top">
            <span class="main-menu-icon">
                <span aria-hidden="true" class="icon icon-grid-big icon-size-medium"></span>
            </span>
            <span class="main-menu-text">Profilim</span>
        </a>
    </li><li class="menu-item-top" style="float: left">
        <a href="/Uye/HesapGecmisi" class="top">
            <span class="main-menu-icon">
                <span aria-hidden="true" class="icon icon-grid-big icon-size-medium"></span>
            </span>
            <span class="main-menu-text">Hesap Geçmişi</span>
        </a>
    </li><li class="menu-item-top" style="float: left">
        <a href="/Uye/Reserve" class="top">
            <span class="main-menu-icon">
                <span aria-hidden="true" class="icon icon-grid-big icon-size-medium"></span>
            </span>
            <span class="main-menu-text">Reservelerim</span>
        </a>
    </li><li class="menu-item-top" style="float: left">
        <a href="/Uye/Istek" class="top">
            <span class="main-menu-icon">
                <span aria-hidden="true" class="icon icon-grid-big icon-size-medium"></span>
            </span>
            <span class="main-menu-text">İsteklerim</span>
        </a>
    </li>
    <li class="menu-item-top">
        <a href="/Uye/Arama" class="top">
            <span class="main-menu-text">Arama</span>
        </a>
    </li>
    <li class="menu-item-top" style="float: left">
        <a href="/Home/AnaSayfa" class="top">
            <span class="main-menu-icon">
                <span aria-hidden="true" class="icon icon-grid-big icon-size-medium"></span>
            </span>
            <span class="main-menu-text">Çıkış</span>
        </a>
    </li>
    <li class="menu-item-top" style="float: right">
        <a href="/Uye/Profil" class="top">
            <span class="main-menu-icon">
                <span aria-hidden="true" class="icon icon-grid-big icon-size-medium"></span>
            </span>
            <span class="main-menu-text">@ViewData["SchoolNumber"]</span>
        </a>
    </li>


}
<div class="row">

    <div class="col-md-12">
        <div class="block">

            <div class="block-content-outer">
                <div class="block-content-inner">
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Adı</th>
                                <th>Yazarı</th>
                                <th>ISBN</th>
                                <th>Yayınevi</th>
                                <th>Kategori</th>
                                <th>Tür</th>
                                <th>Dil</th>

                            </tr>
                        </thead>
                        <tbody>


                            @foreach (MaterialC m in Model)
                            {
                                <tr>
                                    <td>@m.MaterialId</td>
                                    <td>@m.MaterialName</td>
                                    <td>@m.AuthorsName</td>
                                    <td>@m.ISBN</td>
                                    <td>@m.PublisherName</td>
                                    <td>@m.CategoryName</td>
                                    <td>@m.TypeName</td>
                                    <td>@m.LanguageName</td>
                                </tr>

                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>